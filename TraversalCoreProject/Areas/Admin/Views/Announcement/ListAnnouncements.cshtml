@model List<AdminAnnouncementVM>
@{
    ViewData["Title"] = "ListAnnouncements";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Duyurular</h1>


<table class=" table table-hover">
    <tr>
        <td>Başlık</td>
        <td>İçerik</td>
        <td>Oluşturulma Tarihi</td>

    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.Title</td>
    <td>@item.Content</td>
    <td>@item.CreateDate</td>
</tr>
    }
</table>

